{
  "manifest_version": 3,
  "name": "YouTube Time Tracker",
  "version": "1.0",
  "icons": {
    "16": "image.png",
    "32": "image.png",
    "48": "image.png",
    "128": "image.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["https://*.youtube.com/*"],
  "permissions": [
    "storage"
  ]
}
